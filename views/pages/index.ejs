<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Top Mascots</h1>

            <ul>
                <% listaProductos.forEach(function(producto) { %>
                    <li>
                        <strong><%= producto.nombre %></strong>
                        <%= producto.precio %>
                        <%= producto.referencia%>
                    </li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>